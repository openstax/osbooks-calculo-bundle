<entry valign="top" align="left"><m:math><m:mrow><m:mi>x</m:mi><m:mo>=</m:mo><m:mn>1</m:mn><m:mtext>/</m:mtext><m:mn>2 </m:mn></m:mrow></m:math></entry>
<entry valign="top" align="left"><m:math><m:mrow><m:mrow><m:mo>(</m:mo><m:mtext>+</m:mtext><m:mo>)</m:mo></m:mrow><m:mrow><m:mo>(</m:mo><m:mtext>- −</m:mtext><m:mo>)</m:mo></m:mrow><m:mtext>/</m:mtext><m:mo>+</m:mo><m:mo>=</m:mo><m:mtext>- −</m:mtext></m:mrow></m:math></entry>
