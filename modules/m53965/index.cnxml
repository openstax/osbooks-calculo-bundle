  <m:mn>3</m:mn>
  <m:mi>x</m:mi>
  <m:mo>+</m:mo>
  <m:mi>z</m:mi>
  <m:mo>=</m:mo>
  <m:mn>9</m:mn>
  <m:mo>,</m:mo>
  <m:mo>-</m:mo>
  <m:mn>3</m:mn>
  <m:mi>x</m:mi>
  <m:mo>+</m:mo>
  <m:mi>z</m:mi>
  <m:mo>=</m:mo>
  <m:mn>9</m:mn>
  <m:mo>,</m:mo>
  <m:mo>-</m:mo>
  <m:mn>3</m:mn>
  <m:mi>y</m:mi>
  <m:mo>+</m:mo>
  <m:mi>z</m:mi>
  <m:mo>=</m:mo>
  <m:mn>9</m:mn>
  <m:mo>,</m:mo>
  <m:mtext> y </m:mtext>
